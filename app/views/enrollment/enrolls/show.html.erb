<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>과목명</th>
    </tr>
    <tr>
      <th>과목 정보</th>
    </tr>
    <tr>
      <th>과목 내용</th>
    </tr>
  </thead>
  <tbody>
    <% @lecture.each do |lecture| %>
      <tr>
        <td><%= lecture.name %></td>
